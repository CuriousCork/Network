@echo off

:MENU
cls
echo ###############################
echo NETWORK SCRIPT
echo by an idiot with too many hours of life wasted
echo ###############################
echo 1- Ip Config
echo 2- DRIP Goku (Extremely Important for a Computer Check)
echo 3- Renew Your IP Configurations
echo 4 - DNS query
echo 5 - Check how efficient is your machine
echo 6 - Exit
set /p opt="Select any option and then press enter:  "

if %opt% == 1 goto CONFIG
if %opt% == 2 goto Goku
if %opt% == 3 goto IPRENEW
if %opt% == 4 goto DNS
if %opt% == 5 goto ENERGY_EFFICIENT
if %opt% == 6 goto exit
else goto MENU

:CONFIG
ipconfig
pause
goto MENU

:EXIT
echo See ya!
exit                                                                                                                                  

:DNS
set /p domain="Insert Domain: "
set /p dnsserver="Insert DNS Server:"
nslookup %domain% %dnsserver%
pause
goto MENU

:ENERGY_EFFICIENT
powercfg /energy
pause
goto MENU
                                                                                                                                 
:IPRENEW
ipconfig /release
pause
ipconfig /new
pause
goto MENU                                                                                                                                 
                                                                                                                                 
                                                                                                                                 
                                                                                                                                 
                                                                                                                                 
